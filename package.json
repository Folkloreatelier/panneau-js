    "scripts": {
        "storybook": "start-storybook -p 9001 -c .storybook",
        "test": "NODE_ENV=test jest --config ./jest.config.js",
        "bootstrap": "lerna bootstrap",
        "bootstrap:dependencies": "lerna bootstrap --ignore-scripts",
        "clean:package-lock": "lerna exec -- rm -f ./package-lock.json",
        "clean:packages": "lerna run clean",
        "clean": "lerna clean --yes && npm run clean:packages && npm run clean:package-lock",
        "eslint": "eslint '{fields,forms,layouts,lists,modals,packages,previews}/*/src/**/*.{js,jsx}'",
        "docs:styleguide": "npx styleguidist build",
        "docs:gh-pages": "gh-pages -d styleguide",
        "build:storybook": "build-storybook -o docs/storybook",
        "build:schemas": "rsync -avh ./packages/schemas/docs/ ./docs/schemas/",
        "build:gitbook": "gitbook install && gitbook build __docs__ docs/",
        "build:docs": "npm run build:gitbook && npm run build:storybook && npm run build:schemas",
        "build": "lerna run build && npm run build:docs",
        "start": "npm run storybook"
    },
