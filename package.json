{
    "name": "react-panneau",
    "version": "0.1.0",
    "author": {
        "name": "Folklore",
        "email": "info@atelierfolklore.ca"
    },
    "contributors": [
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@atelierfolklore.ca"
        },
        {
            "name": "Nicolas Roy-Bourdages",
            "email": "nrb@atelierfolklore.ca"
        },
        {
            "name": "Hugo St-Onge",
            "email": "hso@atelierfolklore.ca"
        }
    ],
    "main": "lib/index.js",
    "module": "es/index.js",
    "browser": "dist/react-panneau.js",
    "scripts": {
        "test": "jest",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "storybook-deploy": "storybook-to-ghpages",
        "clean:tmp": "rm -rf ./.tmp",
        "clean": "npm run clean:tmp && npm run clean:dist",
        "mkdir:tmp": "mkdir -p ./.tmp",
        "mkdir": "npm run mkdir:tmp && npm run mkdir:dist",
        "mkdir:dist": "mkdir -p ./dist",
        "clean:dist": "rm -rf ./dist",
        "release": "build/release.sh",
        "start": "npm run storybook",
        "lint:dist": "eslint src/**.js",
        "lint": "npm run lint:dist",
        "jscs": "jscs src/**.js",
        "webpack:dist": "node -r babel-register ./node_modules/webpack/bin/webpack --env=dist  --config build/webpack.config.js --progress --colors",
        "webpack": "npm run webpack:dist",
        "scripts:compile:es": "BABEL_ENV=es babel src --out-dir es",
        "scripts:compile:cjs": "BABEL_ENV=cjs babel src --out-dir lib",
        "scripts:compile": "npm run scripts:compile:es && npm run scripts:compile:cjs",
        "scripts:dist": "npm run webpack:dist",
        "scripts": "npm run scripts:dist && npm run scripts:compile",
        "build:js": "npm run lint && npm run scripts",
        "build": "npm run build:files && npm run build:js && npm run clean:tmp",
        "build:files": "npm run clean && npm run mkdir"
    },
    "dependencies": {
        "babel-runtime": "^6.26.0",
        "brace": "^0.10.0",
        "ckeditor": "^4.7.1",
        "classnames": "^2.2.5",
        "color": "^2.0.0",
        "font-awesome": "^4.7.0",
        "lodash": "^4.17.4",
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-ace": "^5.2.0",
        "react-color": "^2.13.5",
        "react-dom": "^15.6.1",
        "react-markdown": "^2.5.0",
        "react-select": "^1.0.0-rc.5"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^3.2.0",
        "@storybook/addon-info": "^3.2.5",
        "@storybook/addon-storyshots": "^3.2.5",
        "@storybook/addons": "^3.2.6",
        "@storybook/channels": "^3.2.0",
        "@storybook/react": "^3.2.4",
        "@storybook/storybook-deployer": "^2.0.0",
        "autoprefixer": "^7.1.2",
        "babel-core": "^6.25.0",
        "babel-eslint": "^7.2.3",
        "babel-jest": "^20.0.3",
        "babel-loader": "^7.1.1",
        "babel-plugin-css-modules-transform": "^1.2.7",
        "babel-plugin-dynamic-import-node": "^1.0.2",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-es2015-spread": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.24.1",
        "bootstrap-sass": "^3.3.7",
        "brfs": "^1.4.3",
        "browser-sync": "^2.18.13",
        "bs-fullscreen-message": "^1.1.0",
        "commander": "^2.11.0",
        "concurrently": "^3.5.0",
        "coveralls": "^2.13.1",
        "css-loader": "^0.28.4",
        "cssnano": "^3.10.0",
        "customizr": "^1.0.0-alpha",
        "domready": "^1.0.8",
        "enzyme": "^2.9.1",
        "eslint": "^3.19.0 || ^4.3.0",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jest": "^20.0.3",
        "eslint-plugin-jsx-a11y": "^5.1.1",
        "eslint-plugin-react": "^7.1.0",
        "expose-loader": "^0.7.3",
        "extract-text-webpack-plugin": "^3.0.0",
        "html-loader": "^0.5.1",
        "html-webpack-plugin": "^2.30.1",
        "imagemin-cli": "^3.0.0",
        "imagemin-mozjpeg": "^6.0.0",
        "imagemin-pngquant": "^5.0.1",
        "imports-loader": "^0.7.1",
        "jest": "^20.0.4",
        "jquery": "^3.2.1",
        "jsdom": "^11.2.0",
        "json-loader": "^0.5.7",
        "node-sass": "^4.5.3",
        "postcss-cli": "^4.1.0",
        "postcss-loader": "^2.0.6",
        "pretty-bytes": "^4.0.2",
        "proxy-middleware": "^0.15.0",
        "raw-loader": "^0.5.1",
        "react-hot-loader": "^3.0.0-beta.7",
        "react-test-renderer": "^15.6.1",
        "sass-loader": "^6.0.6",
        "serve-static": "^1.12.4",
        "strip-ansi": "^4.0.0",
        "style-loader": "^0.18.2",
        "svg-react-loader": "^0.4.4",
        "transform-loader": "^0.2.4",
        "webpack": "^3.5.4",
        "webpack-dev-middleware": "^1.12.0",
        "webpack-hot-middleware": "^2.18.2",
        "webpack-merge": "^4.1.0",
        "webpack-node-externals": "^1.6.0"
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/src/__tests__/setup.js"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/lib/",
            "<rootDir>/es/",
            "<rootDir>/dist/",
            "<rootDir>/src/__tests__/setup.js"
        ],
        "modulePathIgnorePatterns": [
            "<rootDir>/lib/",
            "<rootDir>/dist/",
            "<rootDir>/es/"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
            "\\.(css|less|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
        },
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!**/node_modules/**",
            "!**/vendor/**",
            "!**/__tests__/**",
            "!**/__stories__/**"
        ]
    }
}
