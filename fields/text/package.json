    "main": "lib/index.js",
    "module": "es/index.js",
    "files": [
        "lib",
        "es",
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf dist && rm -rf lib && rm -rf es",
        "webpack:dist": "node -r babel-register ../../node_modules/.bin/webpack --config ./webpack.config.js --progress --colors",
        "webpack": "npm run webpack:dist",
        "babel:es": "BABEL_ENV=es ../../node_modules/.bin/babel src --out-dir es",
        "babel:cjs": "BABEL_ENV=cjs ../../node_modules/.bin/babel src --out-dir lib",
        "babel": "npm run babel:es && npm run babel:cjs",
        "build": "npm run webpack && npm run babel",
        "prepare": "npm run clean && npm run build"
    },
    "dependencies": {
        "babel-runtime": "^6.26.0",
        "ckeditor": "^4.7.3",
        "classnames": "^2.2.5",
        "lodash": "^4.17.4",
        "@react-panneau/form-group": "^0.1.0",
        "@react-panneau/field-locale": "^0.1.0"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^3.3.0-alpha.2",
        "@storybook/addon-info": "^3.3.0-alpha.2",
        "@storybook/addons": "^3.3.0-alpha.2",
        "@storybook/react": "^3.3.0-alpha.2",
        "prop-types": "^15.6.0",
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    },
    "peerDependencies": {
        "prop-types": "^15.6.0",
        "react": ">=15.0.0 || ^16.0.0",
        "react-dom": ">=15.0.0 || ^16.0.0"
    }
}
