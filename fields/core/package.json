{
    "name": "@panneau/field",
    "version": "0.6.30",
    "description": "Field core for Panneau",
    "keywords": [
        "react",
        "javascript",
        "form",
        "dashboard"
    ],
    "homepage": "https://github.com/Folkloreatelier/panneau-js/tree/master/fields/core",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Folkloreatelier/panneau-js.git"
    },
    "author": {
        "name": "Folklore",
        "email": "info@atelierfolklore.ca"
    },
    "contributors": [
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@atelierfolklore.ca"
        },
        {
            "name": "Nicolas Roy-Bourdages",
            "email": "nrb@atelierfolklore.ca"
        }
    ],
    "main": "lib/index.js",
    "module": "es/index.js",
    "files": [
        "lib",
        "es",
        "dist"
    ],
    "scripts": {
        "storybook": "../../node_modules/.bin/start-storybook -p 9008 -c ../../.storybook-package",
        "clean": "rm -rf dist && rm -rf lib && rm -rf es",
        "webpack:dist": "../../node_modules/.bin/webpack --config ./webpack.config.js --progress --colors",
        "webpack": "npm run webpack:dist",
        "babel:es": "BABEL_ENV=es ../../node_modules/.bin/babel src --out-dir es --ignore src/**/*.story.jsx,src/**/*.test.jsx",
        "babel:cjs": "BABEL_ENV=cjs ../../node_modules/.bin/babel src --out-dir lib --ignore src/**/*.story.jsx,src/**/*.test.jsx",
        "babel": "npm run babel:es && npm run babel:cjs",
        "intl": "node ../../build/buildPackageTranslations",
        "build": "npm run babel && npm run intl",
        "prepare": "npm run clean && npm run build",
        "start": "npm run storybook"
    },
    "dependencies": {
        "@babel/runtime": "^7.0.0",
        "@panneau/core": "^0.6.30",
        "classnames": "^2.2.5",
        "fine-uploader": "^5.16.0",
        "fine-uploader-wrappers": "^1.0.1",
        "lodash": "^4.17.4",
        "moment": "^2.21.0",
        "prop-types": "^15.6.0",
        "react-fine-uploader": "^1.0.9",
        "react-intl": "^2.4.0",
        "react-loadable": "^5.5.0",
        "react-markdown": "^3.1.4"
    },
    "devDependencies": {
        "bootstrap": "~4.1.1",
        "react": "^16.7.0",
        "react-dom": "^16.7.0"
    },
    "peerDependencies": {
        "react": ">=15.0.0 || ^16.0.0",
        "react-dom": ">=15.0.0 || ^16.0.0"
    },
    "gitHead": "ec6b2a4882879b80298705db094c6c739e1802ae"
}
