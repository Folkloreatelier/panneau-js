        "@panneau/field-select": "^1.0.0-alpha.0",
        "@panneau/field-slider": "^1.0.0-alpha.0",
        "@panneau/field-text": "^1.0.0-alpha.0",
        "@panneau/field-toggle": "^1.0.0-alpha.0",
        "@panneau/field-url": "^1.0.0-alpha.0",
        "invariant": "^2.2.2",
        "lodash": "^4.17.4",
