{
    "name": "Simple Panneau",

    "layout": {
        "type": "normal",
        "header": {
            "navbar": {
                "items": [
                    {
                        "id": "users",
                        "type": "resource",
                        "resource": "users"
                    },
                    {
                        "id": "user",
                        "type": "user",
                        "position": "right"
                    }
                ]
            }
        },
        "footer": true
    },

    "routes": {
        "resource.index": "/:resource",
        "resource.create": "/:resource/create",
        "resource.store": "/:resource",
        "resource.show": "/:resource/:id",
        "resource.edit": "/:resource/:id/edit",
        "resource.update": "/:resource/:id",
        "resource.destroy": "/:resource/:id"
    },

    "resources": [
        {
            "id": "users",
            "name": "Users",
            "forms": {
                "type": "normal",
                "fields": [
                    {
                        "name": "name",
                        "type": "text",
                        "label": "Name"
                    },
                    {
                        "name": "email",
                        "type": "email",
                        "label": "Email"
                    },
                    {
                        "name": "password",
                        "type": "password",
                        "label": "Password"
                    }
                ]
            }
        }
    ]
}
