{
    "name": "@panneau/core",
    "version": "0.3.48",
    "description": "Core utilities of the Panneau package",
    "keywords": [
        "react",
        "javascript",
        "form",
        "dashboard"
    ],
    "homepage": "https://github.com/Folkloreatelier/panneau-js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Folkloreatelier/panneau-js.git"
    },
    "author": {
        "name": "Folklore",
        "email": "info@atelierfolklore.ca"
    },
    "contributors": [
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@atelierfolklore.ca"
        },
        {
            "name": "Nicolas Roy-Bourdages",
            "email": "nrb@atelierfolklore.ca"
        },
        {
            "name": "Hugo St-Onge",
            "email": "hso@atelierfolklore.ca"
        }
    ],
    "main": "lib/index.js",
    "module": "es/index.js",
    "files": [
        "lib",
        "es",
        "dist"
    ],
    "scripts": {
        "storybook": "../../node_modules/.bin/start-storybook -p 9008 -c ../../.storybook-package",
        "clean": "rm -rf dist && rm -rf lib && rm -rf es",
        "webpack:dist": "node -r babel-register ../../node_modules/.bin/webpack --config ./webpack.config.js --progress --colors",
        "webpack": "npm run webpack:dist",
        "babel:es": "BABEL_ENV=es ../../node_modules/.bin/babel src --out-dir es --ignore story.js,test.js",
        "babel:cjs": "BABEL_ENV=cjs ../../node_modules/.bin/babel src --out-dir lib --ignore story.js,test.js",
        "babel": "npm run babel:es && npm run babel:cjs",
        "copy:json": "rsync -a --include '*/' --include '*.json' --exclude '*' src/ lib/ && rsync -a --include '*/' --include '*.json' --exclude '*' src/ es/",
        "intl": "node ../../build/buildPackageTranslations",
        "build": "npm run webpack && npm run babel && npm run intl && npm run copy:json",
        "prepare": "npm run clean && npm run build",
        "start": "npm run storybook"
    },
    "dependencies": {
        "babel-runtime": "^6.26.0",
        "bootstrap-sass": "^3.3.7",
        "bootstrap.native": "^2.0.21",
        "classnames": "^2.2.5",
        "font-awesome": "^4.7.0",
        "hoist-non-react-statics": "^2.3.1",
        "invariant": "^2.2.2",
        "lodash": "^4.17.4",
        "prop-types": "^15.6.0",
        "react-intl": "^2.4.0",
        "react-redux": "^5.0.6",
        "react-router": "^3.2",
        "react-router-redux": "^4.0.8",
        "redux": "^3.7.2",
        "redux-devtools": "^3.4.1",
        "redux-devtools-dock-monitor": "^1.1.3",
        "redux-devtools-log-monitor": "^1.4.0",
        "redux-promise": "^0.5.3",
        "redux-thunk": "^2.2.0",
        "whatwg-fetch": "^2.0.3"
    },
    "devDependencies": {
        "react": ">=15.0.0 || ^16.0.0",
        "react-dom": ">=15.0.0 || ^16.0.0",
        "react-intl": "^2.4.0"
    },
    "peerDependencies": {
        "react": ">=15.0.0 || ^16.0.0",
        "react-dom": ">=15.0.0 || ^16.0.0"
    }
}
