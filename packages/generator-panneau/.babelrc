{
    "presets": [
        ["env", {
            "modules": "commonjs",
            "targets": {
                "node": "6"
            }
        }]
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        ["transform-runtime", {
            "helpers": true,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }]
    ],
    "ignore": [
        "src/generators/*/templates/**"
    ]
}
